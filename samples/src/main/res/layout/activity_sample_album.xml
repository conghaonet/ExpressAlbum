<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="AlbumUtil.getAllFolders"
        android:textAllCaps="false"
        android:onClick="onClickAlbumUtilGetAllFolder"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="AlbumUtil.getAllImages"
        android:textAllCaps="false"
        android:onClick="onClickAlbumUtiGetAllImages"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="AlbumUtil.getAllVideos"
        android:textAllCaps="false"
        android:onClick="onClickAlbumUtiGetAllVideos"/>


</LinearLayout>
